{
  "name": "tube-vj",
  "version": "0.0.0",
  "engine": {
    "node": "10.15.0",
    "npm": "6.5.0"
  },
  "main": "dist/main/main.js",
  "scripts": {
    "postinstall": "echo 'See: https://github.com/electron/electron/issues/10167#issuecomment-319516597\n' && rm -rf ./node_modules/electron/node_modules/@types/node",
    "start": "tsc -p src/main && too -c 'ng build master --watch' -c 'ng build controller --watch' -c 'electron .'",
    "build": "ng build master && ng build controller",
    "app": "tsc -p src/main && electron .",
    "lint": "ng lint",
    "test": "ng build master && ng build controller && ng lint && ng test --codeCoverage=true --watch=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "googleapis": "^39.2.0",
    "rxjs": "~6.3.3",
    "spectre.css": "^0.5.8",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@otiai10/too": "git+https://git@github.com/otiai10/too.js.git",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "electron": "^4.1.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
